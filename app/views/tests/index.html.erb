<div class="container">
  <h1>Мои тесты</h1>
  <table class="table">
    <tbody>
    <% @tests.each do |test| %>
        <tr>
          <td><%= link_to test.name, test %></td>
          <td>
            <%= link_to edit_test_path(test) do %>
                <span class="glyphicon glyphicon-pencil"></span> Ред.
            <% end %>
          </td>
        </tr>
    <% end %>
    </tbody>
  </table>
  <br>
  <%= link_to 'Добавить тест', new_test_path %>
</div>
